package Test;
class Adder{
	int i;
	char c;
	float f;
	double d;
	void Adder(int a, char c, int b){
		switch (c) {
		case '+':
			i = a+b;
			break;
		case '-':
			i = a-b;
			break;
		case '/':
			i = a/b;
			break;
		case '*':
			i = a*b;
			break;
		default:
			System.out.println("연산자 값이 잘못되었습니다.");
			break;
		}
	}
}

public class Test_2 {
	public static 
	public static void main(String[] args) throws java.io.IOException{
		Adder result = new Adder();
		result.Adder(a, c, b)
	}
}