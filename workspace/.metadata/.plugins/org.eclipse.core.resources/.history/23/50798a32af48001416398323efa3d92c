package Test;
import java.util.Scanner;
//문자열 추가시 next
//정수 추가시 nextint
//스트링 추가시 nextline

public class ekjfs {
	public static void main(String[] args) throws java.io.IOException{
		Scanner scan = new Scanner(System.in);
	float a, b;
	a = scan.nextFloat();
	b = scan.nextFloat();
	System.out.println((a+b)+", "+b);
	}
}
